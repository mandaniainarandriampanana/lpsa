{% trans_default_domain "LpsaAppBundle" %}

{% if paq3seGravite.id is not null %}
    {% set deleteClassSelector = 'paq3seGravite' %}
{% endif %}

{% extends "LpsaAppBundle:Layout:newEdit.html.twig" %}

{% block crud_header_title %}
    {{ 'paq3seGravite.crud_header_title'|trans }}
{% endblock %}
{% block crud_panel_title %}{% if paq3seGravite.id is not null %}{{ 'paq3seGravite.crud_panel_title.edit'|trans }}{% else %}{{ 'paq3seGravite.crud_panel_title.new'|trans }}{% endif %}{% endblock %} 
{% block crud_panel_nav %}
    {% if paq3seGravite.id is not null %}
    <ul class="nav nav-pills">
        <li>
            <a href="{{ path('admin_paq3seGravite_index') }}" class="btn btn-default" style="padding: 6px 12px;"><i class="fa fa-mail-reply"></i> {{ 'form.back'|trans }}</a>
        </li>
    </ul>
    {% endif %}
{% endblock %}
{% block crud_panel_content %}
    {{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" value="{% if paq3seGravite.id is not null %}{{ 'form.update'|trans }}{% else %}{{ 'form.add'|trans }}{% endif %}" class="btn btn-default" />
    {{ form_end(form) }}    
{% endblock %} 